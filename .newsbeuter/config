include "~/.newsbeuter/config.googlereader"
include "~/.newsbeuter/colorschemes/solarized-dark"

browser "chromium %u"

# automatically reload feeds every half hour
auto-reload yes
reload-time 120
reload-threads 4

goto-first-unread yes
goto-next-feed yes

notify-program "/usr/bin/notify-send"

# Google Reader flagging keys
googlereader-flag-share "S"
googlereader-flag-star "b"

bind-key j down all
bind-key k up all
bind-key J next-feed all
bind-key K prev-feed all
